programa {
 real numUm,numDois,numTres,resultado

  funcao inicio() {

  escreva("O valor para viajar para a Alemanha é:R$",numUm)
  leia(numUm)
  escreva("O valor para viajar para a Italia é:R$",numDois)
  leia(numDois)
  escreva("O valor para viajar Portugal é:R$",numTres)
  leia(numTres)
 resultado = (numDois+numTres+numUm)
 escreva("O valor da Eurotrip ficou em:R$"+resultado)

  }
}
